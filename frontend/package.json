{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "lint": "npm run eslint && npm run stylelint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "eslint": "eslint --ext \".js,.ts,.vue\" .",
    "eslint:fix": "eslint --ext \".js,.ts,.vue\" --fix .",
    "stylelint": "stylelint \"**/*.{vue,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{vue,scss}\" --fix",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w ."
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "bulma": "^0.9.4",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.10.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "vite": "^2.9.12"
  }
}
